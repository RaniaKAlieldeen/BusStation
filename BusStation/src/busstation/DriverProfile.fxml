<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="busstation.DriverProfileController">
   <children>
      <BorderPane prefHeight="411.0" prefWidth="608.0">
         <bottom>
            <VBox alignment="CENTER" prefHeight="72.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="LogoutB" mnemonicParsing="false" onAction="#LogoutBClicked" text="Log Out" />
               </children>
            </VBox>
         </bottom>
         <top>
            <VBox alignment="CENTER" prefHeight="71.0" prefWidth="600.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="DriverNameL" prefHeight="17.0" prefWidth="137.0" />
               </children>
            </VBox>
         </top>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Trips Assigned:">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <Region prefHeight="27.0" prefWidth="200.0" />
                  <Label fx:id="ScheduleL" alignment="TOP_LEFT" prefHeight="177.0" prefWidth="195.0" text="Label" />
               </children>
            </VBox>
         </center>
         <left>
            <Region prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </left>
         <right>
            <Region prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
